{
  "name": "nebula-web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "prisma:generate": "prisma generate --schema prisma/platform.prisma && prisma generate --schema prisma/schema.prisma",
    "build": "npm run prisma:generate && prisma db push && prisma db seed && next build",
    "start": "next start",
    "lint": "eslint --fix .",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "analyze": "cross-env ANALYZE=true npm run build",
    "dev:gitpod": "NEXTAUTH_URL=$(gp url 3000)/ npm run dev",
    "cypress:open": "NODE_ENV=test start-server-and-test dev http://localhost:3000 \"cypress open --browser chrome --e2e\"",
    "cypress:run": "NODE_ENV=test start-server-and-test dev http://localhost:3000 \"cypress run --browser chrome --config video=false --e2e\"",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest"
  },
  "prisma": {
    "seed": "ts-node --transpile-only --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.0.6",
    "@mui/icons-material": "^5.3.1",
    "@mui/lab": "^5.0.0-alpha.67",
    "@mui/material": "^5.4.0",
    "@next-auth/prisma-adapter": "^1.0.5",
    "@next/bundle-analyzer": "^13.1.6",
    "@prisma/client": "^4.11.0",
    "@radix-ui/react-checkbox": "^1.0.1",
    "@radix-ui/react-dialog": "^1.0.3",
    "@radix-ui/react-dropdown-menu": "2.0.2",
    "@radix-ui/react-hover-card": "1.0.4",
    "@radix-ui/react-select": "1.2.1",
    "@radix-ui/react-switch": "1.0.1",
    "@react-pdf/renderer": "^3.0.3",
    "@reduxjs/toolkit": "^1.6.0",
    "@tanstack/react-query": "^4.16.1",
    "@tanstack/react-query-devtools": "^4.24.10",
    "@trpc/client": "^10.0.0",
    "@trpc/next": "^10.0.0",
    "@trpc/react-query": "^10.0.0",
    "@trpc/server": "^10.0.0",
    "@vercel/analytics": "^0.1.11",
    "animate.css": "^4.1.1",
    "axios": "^1.3.4",
    "bson": "^4.7.2",
    "daisyui": "^2.50.0",
    "eslint": "^8.8.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "framer-motion": "^6.2.3",
    "fuse.js": "^6.6.2",
    "intersection-observer": "^0.12.2",
    "intro.js": "^6.0.0",
    "intro.js-react": "^0.7.1",
    "next": "^13.1.6",
    "next-auth": "^4.20.1",
    "nodemailer": "^6.8.0",
    "nprogress": "^0.2.0",
    "pdfjs-dist": "^3.3.122",
    "prettier-eslint": "^13.0.0",
    "react": "^18.2.0",
    "react-awesome-reveal": "^4.1.0",
    "react-custom-scrollbars-2": "^4.5.0",
    "react-dom": "^18.2.0",
    "react-intersection-observer": "^9.4.0",
    "react-toastify": "^9.1.1",
    "superjson": "^1.11.0",
    "tailwind-scrollbar-hide": "^1.1.7",
    "tss-react": "^3.3.6",
    "uuid": "^8.3.2",
    "zod": "^3.19.1"
  },
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@jest/globals": "^29.6.1",
    "@types/node": "^17.0.14",
    "@types/nprogress": "^0.2.0",
    "@types/react": "^17.0.39",
    "@types/react-dom": "^18.0.11",
    "@types/uuid": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^5.10.2",
    "autoprefixer": "^10.4.2",
    "cross-env": "^7.0.3",
    "cypress": "^12.4.1",
    "dotenv": "^16.0.3",
    "eslint-config-next": "^12.0.10",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "jest": "^29.6.1",
    "postcss": "^8.4.6",
    "prettier": "^2.3.2",
    "prettier-plugin-tailwindcss": "^0.2.2",
    "prisma": "^4.11.0",
    "react-styleguidist": "^13.0.0",
    "start-server-and-test": "^2.0.0",
    "tailwindcss": "^3.0.18",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.2"
  },
  "overrides": {
    "@react-pdf/renderer": {
      "react": "^18.0.0"
    }
  }
}
