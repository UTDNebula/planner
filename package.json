{
  "name": "nebula-web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "bootstrap": "npm run prisma:generate && npx prisma migrate dev && npm run dev",
    "dev": "next dev --turbopack",
    "prisma:generate": "prisma generate --schema prisma/platform.prisma && prisma generate --schema prisma/schema.prisma",
    "prisma:migrate:deploy": "prisma migrate deploy --schema prisma/schema.prisma",
    "build": "npm run prisma:generate && next build && npm run prisma:migrate:deploy && prisma db seed",
    "start": "next start",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "lint": "next lint --fix",
    "lint:check": "next lint",
    "analyze": "cross-env ANALYZE=true npm run build",
    "dev:gitpod": "cross-env NEXTAUTH_URL=$(gp url 3000)/ npm run dev",
    "cypress:open": "cross-env NODE_ENV=test start-server-and-test dev http://localhost:3000 \"cypress open --browser chrome --e2e\"",
    "cypress:run": "cross-env NODE_ENV=test start-server-and-test start http://127.0.0.1:3000 \"cypress run --browser chrome --config video=false --e2e\"",
    "test": "cross-env NODE_OPTIONS=--experimental-vm-modules jest",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "prisma": {
    "seed": "ts-node --transpile-only --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@mui/icons-material": "^7.1.1",
    "@mui/material": "^7.1.1",
    "@next-auth/prisma-adapter": "^1.0.7",
    "@next/bundle-analyzer": "^15.3.3",
    "@next/third-parties": "^15.3.3",
    "@radix-ui/react-checkbox": "^1.3.2",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-hover-card": "^1.1.14",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-switch": "^1.2.5",
    "@react-pdf/renderer": "^4.3.0",
    "@sentry/nextjs": "^9.25.1",
    "@tanstack/react-query": "^5.80.2",
    "@tanstack/react-query-devtools": "^5.80.2",
    "@trpc/client": "^11.2.0",
    "@trpc/next": "^11.2.0",
    "@trpc/react-query": "^11.2.0",
    "@trpc/server": "^11.2.0",
    "async-mutex": "^0.5.0",
    "eslint": "^9.28.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "framer-motion": "^12.16.0",
    "fuse.js": "^7.1.0",
    "intro.js": "^7.2.0",
    "intro.js-react": "^1.0.0",
    "next": "^15.3.3",
    "next-auth": "^4.24.11",
    "nodemailer": "^6.10.1",
    "nprogress": "^0.2.0",
    "pdfjs-dist": "5.0.375",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hotkeys-hook": "^5.1.0",
    "react-loading-skeleton": "^3.5.0",
    "react-toastify": "^11.0.5",
    "superjson": "^2.2.2",
    "uuid": "^11.1.0",
    "zod": "^3.25.49"
  },
  "devDependencies": {
    "@babel/core": "^7.27.4",
    "@jest/globals": "^29.7.0",
    "@prisma/client": "^6.9.0",
    "@storybook/addon-essentials": "^8.6.14",
    "@storybook/addon-interactions": "^8.6.14",
    "@storybook/addon-links": "^8.6.14",
    "@storybook/addon-onboarding": "^8.6.14",
    "@storybook/blocks": "^8.6.14",
    "@storybook/nextjs": "^8.6.14",
    "@storybook/react": "^8.6.14",
    "@storybook/testing-library": "^0.2.2",
    "@tailwindcss/postcss": "^4.1.8",
    "@types/node": "^22.15.29",
    "@types/nprogress": "^0.2.3",
    "@types/react": "^19.1.6",
    "@types/react-dom": "^19.1.5",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.33.1",
    "cross-env": "^7.0.3",
    "cypress": "^14.4.0",
    "dotenv": "^16.5.0",
    "eslint-config-next": "^15.3.3",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-storybook": "^0.12.0",
    "jest": "^29.7.0",
    "node-fetch": "^3.3.2",
    "postcss": "^8.5.4",
    "prettier": "^3.5.3",
    "prisma": "^6.9.0",
    "start-server-and-test": "^2.0.12",
    "storybook": "^8.6.14",
    "tailwindcss": "^4.1.8",
    "ts-jest": "^29.3.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  }
}
